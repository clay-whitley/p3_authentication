<div class="container">
  <% unless current_user %>
    <h1>Welcome to the app! Please log in or sign up</h1>
  <% else %>
    <h1>Users:</h1>
    <ul>
      <% @users.each do |user| %>
        <li>
          <h4><%= user.name %></h4>
          <p><%= user.email %></p>
        </li>
      <% end %>
    </ul>
  <% end %>
  <%# TODO: 
  1. If user is not signed in - display "Welcome".
  2. If user is signed in then  display all users %>
</div>
